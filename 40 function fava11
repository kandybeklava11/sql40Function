delete from people where id=1;

select id from people;
select * from people where nationality='Canada';
select * from people where age between 25 and 35 and occupation='Engineer';
select * from people where is_Married='true' and has_Children='true';
select * from people where height>170;
select * from people where weight<75;
select avg(age) from people ;
select COUNT(*) AS people_count from people where occupation is not null;
select occupation, COUNT(*) AS people_count from people GROUP BY occupation;
select * from people ORDER BY age ASC LIMIT 1;
select *from people  where email like   '%gmail.com%';
select *from people order by height desc;
select count(*) from people as people_total where nationality='Germany';
select * from people where email like '%com' and phone_number=555;
select COUNT(*) AS people_count from people where occupation is null;
select * from people where is_Married ='false';
select* from people where education like '%Master%';
select * from people where height between 160 and 170;
select * from people where languages like '%English%' and languages like '%China%';
select * from people where is_Married ='false' and has_Children='true';
select *from people where weight>79 or is_Married='true';
select * from people order by age asc, height desc;
select DISTINCT first_name FROM people;
select * from people where nationality like '%Usa%' or nationality like '%Canada%';
select first_name,address from people where address like '%119%';
select  first_name,gender,phone_number from people where phone_number=55 or phone_number=22 or phone_number=80;
update people set occupation='Devoloper' where is_Married=false;
update people set first_name='Murasul',last_name='Muratov',age=100,gender='Male',address='Asanov 33',phone_number=99999,email='@asanov',occupation='Santecnic',nationality='Usbek',education='King',languages='Usbekistan',is_Married=true,has_Children=true,height=187,weight=100 where id=2;
update people set weight=weight+5 where has_Children=false;
delete from people where occupation ='Teacher';
delete from people where age<20;
delete from people where address is null;
select * from people ORDER BY height DESC LIMIT 3;
select * from people where is_Married=false order by height asc limit 1;
select AVG(age)from people where is_married = true;
select  sum(weight) from people where occupation='developer';
select * from people where email!='gmail,com';
select * from people where age=40 and occupation='developer';
select DISTINCT nationality FROM people;