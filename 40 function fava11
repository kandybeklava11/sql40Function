create table people(
    id serial primary key,
        first_name varchar,
        last_name varchar,
    age int,
    gender varchar,
    address varchar,
    phone_number integer,
    email varchar,
    occupation varchar,
    nationality varchar,
    education varchar,
    languages varchar,
    is_Married  boolean,
    has_Children boolean,
    height int,
    weight int



